{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "build:docker": "DOCKER_BUILD=true next build",
    "start": "next start -p 3000",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests specified yet\" && exit 0",
    "test:watch": "echo \"No tests specified yet\"",
    "validate": "npm run typecheck && npm run lint",
    "clean": "rm -rf .next out node_modules/.cache",
    "seed:firestore": "tsx src/lib/mock-seed.ts",
    "build:export": "set NEXT_PUBLIC_BUILD_MODE=export && next build",
    "firebase:deploy": "npm run build:export && firebase deploy --only hosting",
    "firebase:deploy:all": "firebase deploy",
    "docker:build": "docker build -t loni-panchayat:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local loni-panchayat:latest",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
